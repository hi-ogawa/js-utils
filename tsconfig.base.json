{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "noUncheckedIndexedAccess": false,
    "exactOptionalPropertyTypes": false,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"]
  }
}
