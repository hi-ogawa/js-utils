    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@brillout/json-serializer": "^0.5.13",
    "@hiogawa/utils": "workspace:*",
    "devalue": "^5.1.1",
    "fast-check": "^3.23.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "volta": {
    "extends": "../../package.json"
