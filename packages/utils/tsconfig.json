{
  "extends": "@tsconfig/strictest",
  "include": ["src"],
  "compilerOptions": {
    "outDir": "dist/tsc",
    "noEmit": true,
    "declaration": true,
    "noUncheckedIndexedAccess": false,
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext"]
  }
}
